/**
 * 是否为空
 * @param str
 */
exports.isEmpty = (str) => str.length === 0;
/**
 * 是否为空白
 * @param str
 */
exports.isBlank = (str) => exports.trim(str).length === 0;
/**
 * 是否相等
 * @param str1
 * @param str2
 */
